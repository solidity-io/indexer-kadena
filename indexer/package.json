{
  "name": "@kadena-indexer/indexer",
  "description": "Indexer for the Kadena blockchain",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Hackachain team",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@graphile/depth-limit": "^0.3.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/schema": "^10.0.6",
    "@kadena/chainweb-stream-client": "^0.2.2",
    "@kadena/client": "^1.11.1",
    "@kadena/client-utils": "^0.11.0",
    "@kadena/types": "^0.6.2",
    "@types/axios": "^0.14.4",
    "apollo-server-plugin-base": "^3.7.2",
    "axios": "^1.9.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "express": "^4.18.2",
    "graphql": "^16.10.0",
    "graphql-query-complexity": "^1.0.0",
    "graphql-request": "5.0.0",
    "graphql-scalars": "^1.23.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^5.16.0",
    "ip-range-check": "^0.2.0",
    "node-cache": "^5.1.2",
    "papaparse": "^5.5.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "postgraphile": "^4.13.0",
    "postgraphile-plugin-connection-filter": "^2.3.0",
    "prom-client": "^15.1.0",
    "sequelize": "^6.37.0",
    "subscriptions-transport-ws": "^0.11.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/chai": "^4.3.17",
    "@types/cors": "^2.8.17",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.11.17",
    "@types/papaparse": "^5.3.15",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "chai": "^5.1.1",
    "dotenv": "^16.4.4",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "mocha": "^10.7.3",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsc",
    "create:database": "ts-node -r tsconfig-paths/register src/index.ts --database && yarn migrate:up",
    "dev:guards": "ts-node -r tsconfig-paths/register src/index.ts --guards",
    "dev:database": "ts-node -r tsconfig-paths/register src/index.ts --database",
    "dev:backfillPairs": "ts-node -r tsconfig-paths/register src/index.ts --backfillPairs",
    "dev:streaming": "ts-node -r tsconfig-paths/register src/index.ts --streaming",
    "dev:graphql": "ts-node -r tsconfig-paths/register src/index.ts --graphql",
    "dev:hot:graphql": "nodemon -r tsconfig-paths/register src/index.ts --graphql",
    "graphql:generate-types": "npx graphql-codegen",
    "migrate:up": "dotenv -e .env npx sequelize-cli db:migrate",
    "migrate:down": "dotenv -e .env npx sequelize-cli db:migrate:undo",
    "test:unit": "jest tests/unit",
    "test:queries": "dotenv -e .env.testing jest tests/integration/queries/*.test.ts",
    "test:subscriptions": "yarn test:sub:events && yarn test:sub:new-blocks && yarn test:sub:new-blocks-from-depth",
    "test:file": "dotenv -e .env.testing jest",
    "test:smoke": "yarn compose:up && sleep 5 && jest tests/docker/smoke.test.ts && yarn compose:down",
    "test:sub:events": "dotenv -e .env.testing jest tests/integration/subscriptions/events.subscription.test.ts",
    "test:sub:new-blocks": "dotenv -e .env.testing jest tests/integration/subscriptions/new-blocks.subscription.test.ts",
    "test:sub:new-blocks-from-depth": "dotenv -e .env.testing jest tests/integration/subscriptions/new-blocks-from-depth.subscription.test.ts",
    "compose:up": "docker-compose -f docker-compose.yml up -d",
    "compose:down": "docker-compose -f docker-compose.yml down"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
